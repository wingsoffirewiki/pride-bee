{
  "name": "pride-bee",
  "version": "0.0.0",
  "description": "A bot for adding flags to your discord avatar!",
  "main": "dist/index.js",
  "author": "wofwiki <wings.of.fire.fandom.wiki@gmail.com>",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --enable-source-maps .",
    "dev": "nodemon",
    "build": "tsc",
    "pretty": "prettier --check \"src/**/*.ts\"",
    "pretty-fix": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint  -c .eslintrc.json\"src/**/*.ts\"",
    "lint-fix": "eslint --fix -c .eslintrc.json \"src/**/*.ts\""
  },
  "dependencies": {
    "discord.js": "14.7.1",
    "fero-dc": "4.0.0-dev-20",
    "sharp": "0.31.3"
  },
  "devDependencies": {
    "@types/node": "18.14.6",
    "@types/prettier": "2.7.2",
    "@types/sharp": "0.31.1",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.21",
    "prettier": "2.8.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.4.1"
}
