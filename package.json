	"devDependencies": {
		"@ferod/create": "^0.2.1",
		"@types/node": "^20.1.4",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"nodemon": "^2.0.22",
		"prettier": "2.8.8",
		"ts-node": "^10.9.1",
