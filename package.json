{
  "name": "pride-bee",
  "version": "0.0.0",
  "description": "A bot for adding flags to your discord avatar!",
  "main": "dist/index.js",
  "author": "wofwiki <wings.of.fire.fandom.wiki@gmail.com>",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "18.16.0",
    "pnpm": "8.3.1"
  },
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --enable-source-maps .",
    "dev": "nodemon",
    "build": "pnpm swc && pnpm type-check",
    "type-check": "tsc --noEmit",
    "swc": "swc src --copy-files --config-file \".swcrc\" -d \"dist\"",
    "pretty": "prettier --check \"src/**/*.ts\"",
    "pretty-fix": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint  -c .eslintrc.json \"src/**/*.ts\"",
    "lint-fix": "eslint --fix -c .eslintrc.json \"src/**/*.ts\""
  },
  "dependencies": {
    "discord.js": "14.9.0",
    "fero-dc": "4.0.0-dev-21",
    "sharp": "0.32.1"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.56",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "dotenv": "16.0.3",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "pnpm@8.3.1"
}
